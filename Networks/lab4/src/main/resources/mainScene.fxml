<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: green;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.fothlab.MainController">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
         <children>
            <HBox alignment="TOP_CENTER" prefHeight="38.0" prefWidth="600.0" spacing="100.0">
               <children>
                  <Button fx:id="newGameButton" mnemonicParsing="false" onMouseClicked="#onNewGameButtonPressed" text="Start new game" textAlignment="CENTER">
                     <font>
                        <Font name="Roboto Light" size="12.0" />
                     </font>
                  </Button>
                  <Button fx:id="joinGameButton" mnemonicParsing="false" onMouseClicked="#onJoinGameButtonPressed" text="Join game">
                     <font>
                        <Font name="Roboto Light" size="12.0" />
                     </font>
                  </Button>
                  <Button fx:id="joinGameAsViewerButton" mnemonicParsing="false" onMouseClicked="#onJoinGameAsViewerButtonPressed" text="View mode">
                     <font>
                        <Font name="Roboto Light" size="12.0" />
                     </font>
                  </Button>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
      <Label fx:id="mainSceneTitle" text="Snake" textAlignment="CENTER" StackPane.alignment="TOP_CENTER">
         <font>
            <Font name="Gadugi Bold" size="48.0" />
         </font>
      </Label>
   </children>
</StackPane>
